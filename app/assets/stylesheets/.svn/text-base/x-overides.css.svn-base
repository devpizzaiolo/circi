/* MEDIA BASED LAYOUT */


@media (max-width: 344px) {
	.mobile-logo-holder {
		text-align: left;
		padding-left: 12px;
	}
}

@media (max-width: 767px) {
	
	.media-object {
		max-width:  200px;
	}
  
	div#homepage_courousel_header_1 {
		background: url(/system/uploads/pizza_preset/pizza_image_angled/83/size_800_product_image.jpg) no-repeat scroll center center;
	}

	div#homepage_courousel_header_2 {
		background: url(/system/uploads/pizza_preset/pizza_image_angled/72/size_800_product_image.jpg) no-repeat scroll center center;
	}

	div#homepage_courousel_header_3 {
		background: url(/system/uploads/pizza_preset/pizza_image_angled/57/size_800_product_image.jpg) no-repeat scroll center center;
	}
	
	h1 {
		font-size: 24px;
		line-height: 32px;
	}
	
	h2 {
		font-size: 21px;
		line-height: 26px;
	}
	
	h3 {
		font-size: 18px;
		line-height: 24px;
		margin-top: 6px;
		margin-bottom: 6px;
	}
	
	div.page-header {
		margin-bottom: 6px;
		padding: 0px;
	}
	
	.align-price {
		float: right;
	}
	
	.order-item-row {
		margin-bottom: 8px;
	}
	
	.order-item-row div {
		padding-left: 6px;
		padding-right: 6px;
		background-color: #f4f4ee;
	}
	
	.order-item-row div:first-child {
		padding-top: 6px;
	}
	
	.order-item-row div:last-child {
		padding-bottom: 6px;
	}
	
	body {
		padding:  0px;
	}
	
	.container#content {
		padding: 10px;
	}
	
	.modal {
		top: 100px;
		margin-top: 10px;
	}
	
}

@media (max-width: 767px) {
	
	body.homepage, html.homepage {
		background: url(/assets/home_page/phone-bg.jpg) center top;
	}
	
	.homepage div.top-bit {
		box-shadow: none;
	}
	
	.modal {
		top: 80px;
	}
	
}


@media (min-width: 768px) and (max-width: 979px) {
  	
	div.container#content {
		border: 0px;
		padding: 10px;
	}
	
	img.main_logo {
		width: 300px;
		min-width: 300px;
		margin-left: -16px;
		margin-top: -24px;
	}
	
	div.header {
		height: 150px;
	}
	
}



@media (max-width: 1027px) {

	/* for smaller laptop screens - lose the border */
  
	div.container#content {
		padding: 10px;
		border-style: none;
		border-width: 0px;
	}
	
}





